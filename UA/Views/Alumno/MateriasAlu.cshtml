@model List<UA.Models.Historial>

<h1>Materias</h1>

<div>
    <table>
        <tr>
            <th>ID</th>
            <th>Fecha</th>
            <th>Nota</th>
        </tr>
        @foreach (var materia in Model)
        {
            <tr>
                <td>materia.IDMateria</td>
                <td>materia.Fecha</td>
                <td>materia.Nota</td>
            </tr>
        }
    </table>

</div>

<br />

