
@model List<UA.Models.Inscripcion>

<h1>Materias</h1>

<div>
    <table>
        <tr>
            <th>ID</th>
            <th>Fecha</th>
            <th>Nota</th>
        </tr>
        @foreach (var materia in Model)
        {
            <tr>
                <td>@materia.IDMateria</td>
                <td>@materia.Fecha</td>
                <td>@materia.Nota</td>
            </tr>
        }
    </table>

</div>
<br />
<p><a href="https://localhost:44321/Alumno/NuevaInscripcion" class="btn btn-lg">Inscribirse</a></p>

<br />
<p><a href="https://localhost:44321/Alumno/CrearAlu" class="btn btn-lg">Volver</a></p>
